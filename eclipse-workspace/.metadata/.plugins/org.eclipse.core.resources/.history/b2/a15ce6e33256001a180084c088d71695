/* test.edl - Top EDL file. */

enclave {
    
    /* 
     * ocall_test_sample - invokes OCALL to display string buffer inside the enclave.
     *  [in]: copy the string buffer to App outside.
     *  [string]: specifies 'str' is a NULL terminated buffer.
     */
    untrusted {
        void ocall_test_sample([in, string] const char *str);
    };


    trusted {
    	    public int ecall_test_sample();
    };
};
